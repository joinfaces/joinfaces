        <check.dir>src/checkconfig</check.dir>
        <checkstyle.dir>${check.dir}/checkstyle</checkstyle.dir>
        <pmd.dir>${check.dir}/pmd</pmd.dir>
        <findbugs.dir>${check.dir}/findbugs</findbugs.dir>
        
        <requireMaven.version>3.1.1</requireMaven.version>
        <requireJava.version>[1.8,)</requireJava.version>
        
        <findbugs-maven-plugin.version>3.0.5</findbugs-maven-plugin.version>
        <findbugs.excludeFilterFile>${findbugs.dir}/findbugs-exclude.xml</findbugs.excludeFilterFile>
        <jdepend-maven-plugin.version>2.0</jdepend-maven-plugin.version>
        <maven-pmd-plugin.version>3.8</maven-pmd-plugin.version>
        <pmd.ruleset>${pmd.dir}/pmd-ruleset.xml</pmd.ruleset>
        <pmd.excludeFromFailureFile />
        <cpd.excludeFromFailureFile />
        <maven-enforcer-plugin.version>1.4.1</maven-enforcer-plugin.version>
        <no-package-cycles-enforcer-rule.version>1.0.8</no-package-cycles-enforcer-rule.version>
        <maven-checkstyle-plugin.version>2.17</maven-checkstyle-plugin.version>
        <checkstyle.version>8.2</checkstyle.version>
        <checkstyle.configLocation>${checkstyle.dir}/checkstyle.xml</checkstyle.configLocation>
        <checkstyle.suppressionsLocation>${checkstyle.dir}/checkstyle-suppressions.xml</checkstyle.suppressionsLocation>
        <checkstyle.headerLocation>${checkstyle.dir}/checkstyle-header.txt</checkstyle.headerLocation>
        <checkstyle.excludes />
        <duplicate-finder-maven-plugin.version>1.2.1</duplicate-finder-maven-plugin.version>
        <duplicate.ignoredResourcePattern>about.html</duplicate.ignoredResourcePattern>
        <duplicate.ignoredClassPattern>none</duplicate.ignoredClassPattern>
        
        <maven-surefire-report-plugin.version>2.20</maven-surefire-report-plugin.version>

        <jacoco-maven-plugin.version>0.8.1</jacoco-maven-plugin.version>
        <jacoco.totalBranchRate>1.00</jacoco.totalBranchRate>
        <jacoco.totalLineRate>1.00</jacoco.totalLineRate>
        <jacoco.packageBranchRate>1.00</jacoco.packageBranchRate>
        <jacoco.packageLineRate>1.00</jacoco.packageLineRate>
        <jacoco.branchRate>1.00</jacoco.branchRate>
        <jacoco.lineRate>1.00</jacoco.lineRate>

        <argLine>-Xms32m -Xmx256m</argLine>
        <findbugs.jvmArgs>-Xms32m -Xmx256m</findbugs.jvmArgs>
        
        <maven-project-info-reports-plugin.version>2.9</maven-project-info-reports-plugin.version>
        <maven-jxr-plugin.version>2.5</maven-jxr-plugin.version>
        <maven-changelog-plugin.version>2.3</maven-changelog-plugin.version>        
